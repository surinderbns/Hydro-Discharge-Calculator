            style="@style/style11"
            android:layout_marginTop="10dp"
            android:text="@string/str_3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/tv_2"
            app:layout_constraintHorizontal_bias="0"
            app:layout_constraintStart_toStartOf="@+id/tv_2"
            app:layout_constraintTop_toBottomOf="@+id/tv_2"
            app:layout_constraintVertical_bias="0" />

        <TextView
            android:id="@+id/tv_4"
            style="@style/style11"
            android:layout_marginTop="10dp"
            android:text="@string/str_4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/tv_3"
            app:layout_constraintHorizontal_bias="0"
            app:layout_constraintStart_toStartOf="@+id/tv_3"
            app:layout_constraintTop_toBottomOf="@+id/tv_3"
            app:layout_constraintVertical_bias="0" />

        <TextView
            android:id="@+id/tv_5"
            style="@style/style11"
            android:layout_marginTop="10dp"
            android:text="@string/str_5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/tv_4"
            app:layout_constraintHorizontal_bias="0"
            app:layout_constraintStart_toStartOf="@+id/tv_4"
            app:layout_constraintTop_toBottomOf="@+id/tv_4"
            app:layout_constraintVertical_bias="0" />

        <TextView
            android:id="@+id/tv_6"
            style="@style/style11"
            android:layout_marginTop="10dp"
            android:text="@string/str_6"
