            style="@style/style11"

            android:layout_marginTop="10dp"
            android:background="#DD2C00"
            android:text="@string/calc2"
            android:textColor="#ffff"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/tv_r11"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tv_comm"
            app:layout_constraintTop_toBottomOf="@+id/tv_comm"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/tv_r31"
            style="@style/style11"
            android:layout_marginTop="10dp"
            android:background="#DD2C00"
            android:text="@string/calc3"
            android:textColor="#ffff"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/tv_r21"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tv_r21"
            app:layout_constraintTop_toBottomOf="@+id/tv_r21"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/tv_r41"
            style="@style/style11"
            android:layout_marginTop="10dp"
            android:background="#DD2C00"
            android:text="@string/calc4"
            android:textColor="#ffff"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/tv_r31"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tv_r31"
            app:layout_constraintTop_toBottomOf="@+id/tv_r31"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/title_1"
            android:layout_width="300dp"
            android:layout_height="40dp"
            android:background="#DD2C00"
            android:gravity="center"
            android:paddingLeft="4dp"
            android:paddingRight="4dp"
            android:text="@string/title"
            android:textColor="#ffff"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/tv_1"
            style="@style/style11"
            android:layout_marginTop="10dp"
            android:text="@string/str_1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0"
            app:layout_constraintStart_toStartOf="@+id/title_1"
            app:layout_constraintTop_toBottomOf="@+id/title_1"
            app:layout_constraintVertical_bias="0" />

        <TextView
            android:id="@+id/tv_2"
            style="@style/style11"
            android:layout_marginTop="10dp"
            android:text="@string/str_2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/tv_1"
            app:layout_constraintHorizontal_bias="0"
            app:layout_constraintStart_toStartOf="@+id/tv_1"
            app:layout_constraintTop_toBottomOf="@+id/tv_1"
