            app:layout_constraintEnd_toEndOf="@+id/tv_5"
            app:layout_constraintHorizontal_bias="0"
            app:layout_constraintStart_toStartOf="@+id/tv_5"
            app:layout_constraintTop_toBottomOf="@+id/tv_5"
            app:layout_constraintVertical_bias="0" />

        <TextView
            android:id="@+id/tv_7"
            style="@style/style11"
            android:layout_marginTop="10dp"
            android:text="@string/str_7"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/tv_6"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tv_6"
            app:layout_constraintTop_toBottomOf="@+id/tv_6"
            app:layout_constraintVertical_bias="0.0" />

        <EditText
            android:id="@+id/et_1"
            style="@style/style2"
            android:layout_marginTop="27dp"
            android:ems="10"
            android:inputType="numberDecimal"
            android:hint="@string/ip_1"
            app:layout_constraintBottom_toBottomOf="@+id/tv_1"
            app:layout_constraintEnd_toEndOf="@+id/title_1"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/tv_1"
            app:layout_constraintTop_toTopOf="@+id/tv_1"
            app:layout_constraintVertical_bias="1.0"
            android:autofillHints="15000" />

        <EditText
            android:id="@+id/et_2"
            style="@style/style2"
            android:layout_marginTop="42dp"
            android:layout_marginBottom="4dp"
            android:ems="10"
            android:hint="@string/ip_2"
            android:inputType="numberDecimal"
            app:layout_constraintBottom_toBottomOf="@+id/tv_2"
            app:layout_constraintEnd_toEndOf="@+id/et_1"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/et_1"
            app:layout_constraintTop_toTopOf="@+id/tv_2"
            app:layout_constraintVertical_bias="0.913"
            android:autofillHints="@string/ip_2" />

        <EditText
            android:id="@+id/et_3"
            style="@style/style2"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:ems="10"
            android:hint="@string/ip_3"
            android:inputType="numberDecimal"
            app:layout_constraintBottom_toBottomOf="@+id/tv_3"
            app:layout_constraintEnd_toEndOf="@+id/et_2"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/et_2"
            app:layout_constraintTop_toTopOf="@+id/tv_3"
            app:layout_constraintVertical_bias="1.0"
            android:autofillHints="@string/ip_3" />

        <EditText
            android:id="@+id/et_4"
            style="@style/style2"
            android:layout_marginTop="4dp"
            android:ems="10"
            android:hint="@string/ip_4"
            android:inputType="numberDecimal"
            app:layout_constraintBottom_toBottomOf="@+id/tv_4"
            app:layout_constraintEnd_toEndOf="@+id/et_3"
            app:layout_constraintStart_toStartOf="@+id/et_3"
            app:layout_constraintTop_toTopOf="@+id/tv_4"
            android:autofillHints="@string/ip_4" />

        <EditText
            android:id="@+id/et_5"
            style="@style/style2"
            android:background="@drawable/red_border"
            android:layout_marginTop="4dp"
            android:ems="10"
            android:inputType="numberDecimal"
            android:hint="@string/ip_5"
            app:layout_constraintBottom_toBottomOf="@+id/tv_5"
            app:layout_constraintEnd_toEndOf="@+id/et_4"
            app:layout_constraintStart_toStartOf="@+id/et_4"
            app:layout_constraintTop_toTopOf="@+id/tv_5"
            android:autofillHints="@string/ip_5" />

        <EditText
            android:id="@+id/et_6"
            style="@style/style2"
            android:ems="10"
            android:importantForAutofill="no"
            android:inputType="numberDecimal"
            android:hint="@string/ip_6"
            android:background="@drawable/red_border"
            app:layout_constraintBottom_toBottomOf="@+id/tv_6"
            app:layout_constraintEnd_toEndOf="@+id/et_5"
            app:layout_constraintStart_toStartOf="@+id/et_5"
            app:layout_constraintTop_toTopOf="@+id/tv_6" />

        <EditText
            android:id="@+id/et_7"
            style="@style/style2"
            android:layout_marginStart="3dp"
            android:layout_marginLeft="3dp"
            android:layout_marginTop="70dp"
            android:layout_marginEnd="52dp"
            android:layout_marginRight="52dp"
            android:ems="10"
            android:importantForAutofill="no"
            android:inputType="numberDecimal"
            android:hint="@string/ip_7"
            android:background="@drawable/red_border"
            app:layout_constraintBottom_toBottomOf="@+id/tv_7"
            app:layout_constraintEnd_toEndOf="@+id/et_6"
            app:layout_constraintHorizontal_bias="0.054"
            app:layout_constraintStart_toStartOf="@+id/et_6"
            app:layout_constraintTop_toTopOf="@+id/tv_7"
            app:layout_constraintVertical_bias="1.0" />

        <Button
