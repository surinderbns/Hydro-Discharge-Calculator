            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@drawable/gradient"
            android:text="@string/calc"
            android:textColor="#ffff"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/et_7"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="@+id/tv_7"
            app:layout_constraintTop_toBottomOf="@+id/tv_7"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/tv_r12"
            style="@style/style2"
            android:background="@drawable/red_border"
            app:layout_constraintBottom_toBottomOf="@+id/tv_r11"
            app:layout_constraintEnd_toEndOf="@+id/et_7"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/tv_r11"
            app:layout_constraintTop_toTopOf="@+id/tv_r11"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/tv_r22"
            style="@style/style2"

            app:layout_constraintBottom_toBottomOf="@+id/tv_r21"
            app:layout_constraintEnd_toEndOf="@+id/tv_comm"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@+id/tv_r12"
            app:layout_constraintTop_toTopOf="@+id/tv_r21"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/tv_r32"
            style="@style/style2"
            android:layout_marginTop="12dp"
            app:layout_constraintBottom_toBottomOf="@+id/tv_r31"
            app:layout_constraintEnd_toEndOf="@+id/tv_r22"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@+id/tv_r22"
            app:layout_constraintTop_toTopOf="@+id/tv_r31"
            app:layout_constraintVertical_bias="1.0" />

        <TextView
            android:id="@+id/tv_r42"
            style="@style/style2"
            app:layout_constraintBottom_toBottomOf="@+id/tv_r41"
            app:layout_constraintEnd_toEndOf="@+id/tv_r32"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/tv_r41"
            app:layout_constraintTop_toTopOf="@+id/tv_r41"
            app:layout_constraintVertical_bias="1.0" />

        <TextView
            android:id="@+id/tv_r11"
            style="@style/style11"
            android:layout_marginTop="10dp"
            android:background="#DD2C00"
            android:text="@string/calc1"
            android:textColor="#ffff"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/tv_7"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tv_7"
            app:layout_constraintTop_toBottomOf="@+id/btn_1"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/tv_comm"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="@string/info_1"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/tv_r12"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tv_r11"
            app:layout_constraintTop_toBottomOf="@+id/tv_r11"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:text="@string/info_2"
            android:textColor="#DD2C00"
            android:paddingLeft="5dp"
            android:paddingRight="5dp"
            android:background="@color/common_google_signin_btn_text_light_disabled"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/tv_r42"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/tv_r41"
            app:layout_constraintTop_toBottomOf="@+id/tv_r41"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:background="@color/colorPrimaryDark"
            android:text="@string/info_3"
            android:textColor="#ffff"
            android:paddingStart="5dp"
            android:paddingEnd="5dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView3"
            app:layout_constraintVertical_bias="1.0" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:hint="@string/ad_area"
            app:layout_constraintBottom_toTopOf="@+id/textView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView2"
            app:layout_constraintVertical_bias="1.0" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</ScrollView>